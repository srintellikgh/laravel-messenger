{
    "name": "lexxyungcarter/chatmessenger",
    "description": "Simple one-to-one/group chat messaging tool for Laravel 5 with Pusher",
    "keywords": ["laravel", "chat", "messenger", "messaging", "user messaging", "chat", "pusher"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lexx YungCarter",
            "email": "lexxyungcarter@gmail.com",
            "homepage": "https://github.com/lexxyungcarter",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/config": ">=5.5 <=6.*",
        "illuminate/support": ">=5.5 <=6.*",
        "illuminate/database": ">=5.5 <=6.*"
    },
    "require-dev": {
        "adamwathan/faktory": "0.3.*",
        "friendsofphp/php-cs-fixer": "^2.5",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lexx\\ChatMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lexx\\ChatMessenger\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lexx\\ChatMessenger\\ChatMessengerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
